{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "server/db2-server.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/server/db2-server.js"
    },
    {
      "source": "/:path*",
      "destination": "/dist/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
