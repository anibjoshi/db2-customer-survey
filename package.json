{
  "name": "zora-customer-survey",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "cd server && npm install",
    "dev": "vite",
    "dev:server": "cd server && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "tsc && vite build && cd server && npm install --production",
    "build:client": "tsc && vite build",
    "build:server": "cd server && npm install --production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "cd server && npm start"
  },
  "dependencies": {
    "@carbon/charts": "^1.20.0",
    "@carbon/charts-react": "^1.20.0",
    "@carbon/icons-react": "^11.0.0",
    "@carbon/react": "^1.50.0",
    "@carbon/styles": "^1.50.0",
    "d3": "^7.8.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "sass": "^1.69.5",
    "terser": "^5.24.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
